
    <h1>Results</h1>
    <div class="row">
        <div class="col-sm-6">
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Location</th>
                        <th>Favorite Stack</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>@ViewBag.Name</td>
                        <td>@ViewBag.location</td>
                        <td>@ViewBag.FavStack</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <br>
    <div class='container mt-5'>
        <p>@ViewBag.Comment</p>
    </div>
